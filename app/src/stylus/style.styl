.overflow-review
	max-height: 480px
	overflow-y scroll
	margin-bottom 2em !important
	+media(1024px)
		overflow-y hidden
		max-height 100%
	

.review
	margin-bottom 2em
	&:nth-child(-n+2)
		+media(1024px)
			display block !important
	+media(1024px)
		display none
		padding 15px
		border: 1px solid #c1c1c1
		background-color: rgba(7, 106, 189, 0)
	p
		margin 10px 0
	&--header
		display flex
		p
			text-shadow: 0 1px 0 rgba(255, 255, 255, 0.7)
			color: #333333
			font-size: 16px
			font-weight: 700
		&__date
			opacity: 0.5
			font-size: 14px
			font-weight: 300
			&::before
				content ''
				display inline-block
				background url('../img/review--time.png')
				width 15px
				height 14px
				margin 0 .2em 0 1em
	&--form
		margin-top 2em
		display flex
		justify-content flex-end
		flex-wrap wrap
		+media(1200px)
			flex-direction column
		.left
			margin-right 2em
			+media(800px)
				margin-right 0
		input,
		textarea
			&::placeholder
				color: #333
				opacity .6
				transition: 0.5s ease all
			&:focus
				&::placeholder
					color: transparent
					transition: 0.5s ease all		
		&__name
			background: url("../img/form__name.png") 95% center no-repeat !important
		&__mail
			background: url("../img/form__mail.png") 95% center no-repeat !important
		input
			width: 359px
			height: 43px
			border-radius: 1px 2px 2px 1px
			border: 1px solid #d9d9d9
			display block
			margin-bottom 20px
			color #333
			padding 18px 16px
			+media(800px)
				width 100%
			&[type="submit"]
				width: 220px
				height: 43px
				text-shadow: 0 1px 0 rgba(255, 255, 255, 0.55)
				color: #076abd
				font-family: "Open Sans"
				font-size: 14px
				font-weight: 600
				padding 0
				box-shadow: 0 1px 0 rgba(33, 111, 178, 0.67), inset 0 -1px 0 rgba(0, 0, 0, 0.17)
				border-radius: 1px 2px 2px 1px
				background: #f7e28b
				transition: 0.5s ease all	
				+media(1024px)
					margin-top 1em
				&:hover
					background: #e5ce8c
					color #333
		&__message
			width: 751px
			height: 106px
			border-radius: 1px 2px 2px 1px
			border: 1px solid #d9d9d9
			background-color: rgba(255, 255, 255, 0)
			color #333
			padding 19px 17px
			+media(800px)
				width 100%
.float
	&__left
		float left
	&__right
		float right
.clear
	clear both
.tabs
	&__faq
		padding-top 1em
		display flex
		+media(1024px)
			flex-direction column
			align-items center
		&--item
			width 100%
		ol
			counter-reset: myCounter
			padding-left 2em
		li
			list-style: none
			margin 13px 0
			&.ui-state-active
				a
					color: #076abd
			a
				font-size 14px
				text-decoration none
				color: #1e1e1e
				&:focus
					outline 0
			&:before
				counter-increment: myCounter
				content: counter(myCounter) '.'
				text-shadow: 0 1px 0 rgba(255, 255, 255, 0.7)
				color: #076abd
				font-family: "Open Sans"
				font-size: 15px
				font-weight: 700
				line-height: 18px
				margin-right 1em
				margin-left -2em

		&__header
			flex-shrink 0
			margin-right 5%
			max-width 25%
			padding-left 2em
			&.lists
				+media(1024px)
					width 100%
					max-width 100%
					padding-left 4em
					margin-bottom 2em
			.top
				color: #076abd
				font-family: "Open Sans"
				font-size: 20px
				font-weight: 700
				text-transform: uppercase
				margin-bottom 1em
.accordeon 
	border-top: none
	margin: 10px 0
	float: left
	width: 100%
	position: relative
	a 
		display: block
		text-decoration: none
.answer 
	color: #555
	border: 1px solid #dcdcdc
	margin-top: -18px
	border-top: 2px solid #FFF
	border-radius: 6px
	padding 20px 18px
	padding-top 0
	position relative
	&::after
		content ''
		width 100%
		height 10px
		display block
		position absolute
		left 0
		top 0
		background: #fff
.ac_head
	font-size 12pt
	text-transform uppercase
	padding 19px 24px
	cursor pointer
	border: 1px solid #dcdcdc
	background-color: rgba(7, 106, 189, 0)
	margin 10px 0
	border-radius 6px
	color: #333333
	font-size: 14px
	font-weight: 700
	line-height: 22px
	position relative
	&.js_up
		.ac_head--array
			transform rotate(90deg)
	&--array
		position absolute
		right 1em
		transition .5s ease all
.ac_head::after
	content: ''
	display: block
	width: 10px
	height: 6px
	float: right
	margin-top: 5px
.ac_head.js_up::after
	background-position: 0 6px
	
