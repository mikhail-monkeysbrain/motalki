media(maxWidth)
	@media screen and (max-width: maxWidth)
		{block}
*
	padding 0
	margin 0
	box-shadow border-box
	font-family: 'Open Sans'
	
.flex
	display flex
	justify-content center
	align-items center
	flex-wrap wrap

.prew
	display flex
	flex-shrink 0
	max-width 42%
	width 459px
	flex-wrap wrap
	justify-content center
	align-items center
	+media(1200px)
		width 100%
		max-width 100%
	img
		width auto
	&__big
		max-width: 385px
		width 100%
		+media(1200px)
			max-width 60%
			text-align center
	&__small
		width 30%
		max-width 81px
		margin-right 13px
		margin-top 20px
		+media(480px)
			max-width 50px
	&--bottom
		width 460px
		display flex
		justify-content flex-start
		align-items center
		+media(1200px)
			justify-content center
		
.product-info 
	overflow hidden
	margin 0 0 5px 0
	padding 0
	display flex
	align-items center
	flex-shrink 0
	+media(1200px)
		width 100%
		justify-content space-around
		margin-top 2em
