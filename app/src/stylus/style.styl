		background: url(../image/warranty-bg.png) center 112px no-repeat
	.attributes,
	.banner-left
		flex-shrink 0 
	.banner-left h3
		top 0
	.price 
		.rub
			margin 0
	.advantages
		ul
			li
				background: url(../image/pi-book.png) 4px 8px no-repeat
	.delivery, .payment, .warranty
		padding: 20px
		border: 1px solid #e8e8e8
		h3
			width 130px
			background transparent
			position relative
			margin 0
			padding 0
			left 0
			top 0

.tab
	display flex
	&__header
