.owl
	&-carousel
		position relative
	&-nav
		display flex
		justify-content space-between
		width calc(100% + 36px + 20px)
		position absolute
		left calc(-16px - 10px)
		height 0
		top calc(50% - 19px - 1em)
	&-dots
		display: flex
		width: initial
		margin: 1em auto
		justify-content: flex-start
		position: relative
		margin-left 80px
	&-prev
		width 18px
		height 35px
		background url('../img/next.png')
		transform rotate(180deg)
		&:hover
			background url('../img/next.png')
			transform rotate(180deg)
	&-next
		width 18px
		height 35px
		background url('../img/next.png')
	&-dot
		width 10px
		height 10px
		margin 5px 10px
		background #ababab
		border-radius 50%
		transition .5s ease
		&:hover
			background #cbcbcb
		&.active
			width 10px
			height 10px
			margin 5px 10px
			background #2a82cc
			border-radius 50%
	&-item
		padding 0 80px

.slider
